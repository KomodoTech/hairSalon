<div class="jumbotron">
    <div class="row">
        <div class="col-sm-6">
            <h4>Clients</h4>
            <ul>
                {% for client in all_clients %}
                    <li><a href="client/{{ client.getId }}">{{ client.getName }}</a></li>
                {% endfor %}
            </ul>
        </div>
        <div class="col-sm-6">
            <h4>Stylists</h4>
            <ul>
                {% for stylist in all_stylists %}
                    <li><a href="stylist/{{ stylist.getId }}">{{ stylist.getName }}</a></li>
                {% endfor %}
            </ul>
        </div>
    </div>
</div>
